
# test and dev model CMM


AT_SETUP([CMM: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/CMM/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/CMM && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

