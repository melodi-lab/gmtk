
# test and dev model karim


AT_SETUP([karim: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/karim/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/karim && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

