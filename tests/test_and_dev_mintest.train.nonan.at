# https://j.ee.washington.edu/trac/gmtk/ticket/161
AT_XFAIL_IF([true])
AT_CHECK([(cd $TEST_AND_DEV/mintest.train.nonan && ulimit -v 8000000 && ulimit -t 120 && ./infocommand)], [], [ignore], [ignore])
