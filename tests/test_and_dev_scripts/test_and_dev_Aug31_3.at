AT_SKIP_IF([test ! -x $TEST_AND_DEV/Aug31_3/infocommand])
AT_CHECK([(cd $TEST_AND_DEV/Aug31_3 && ulimit -v 8000000 && ulimit -t 120 && ./infocommand)], [], [ignore], [ignore])
AT_CLEANUP

AT_SETUP([Aug31_3: jtcommand])
