
# test and dev model Aug31_3


AT_SETUP([Aug31_3: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/Aug31_3/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/Aug31_3 && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

