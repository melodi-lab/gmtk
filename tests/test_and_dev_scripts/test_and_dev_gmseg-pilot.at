AT_SKIP_IF([test ! -x $TEST_AND_DEV/gmseg-pilot/vitcommand])
AT_CHECK([(cd $TEST_AND_DEV/gmseg-pilot && ulimit -v 8000000 && ulimit -t 120 && ./vitcommand)], [], [ignore], [ignore])
AT_CLEANUP
