
# test and dev model staka/hmm


AT_SETUP([staka/hmm: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/staka/hmm/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/staka/hmm && ulimit -v 8000000 &&            ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

