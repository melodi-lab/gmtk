
# test and dev model thad_july


AT_SETUP([thad_july: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/thad_july/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/thad_july && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

