
# test and dev model polyphase/10.2phase


AT_SETUP([polyphase/10.2phase: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/polyphase/10.2phase/tricommand])
AT_CHECK([for f in `ls $TEST_AND_DEV/polyphase/10.2phase/PARAMS/*.bz2`; do \
            if test ! -f ${f%.bz2}; then bunzip2 -k $f; fi \
          done], [],[ignore],[ignore])
AT_CHECK([(cd $TEST_AND_DEV/polyphase/10.2phase && ulimit -v 8000000 &&            ulimit -t 120 && ./tricommand -reSect)], [], [ignore], [ignore])
AT_CLEANUP

