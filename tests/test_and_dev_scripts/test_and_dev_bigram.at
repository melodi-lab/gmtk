
# test and dev model bigram


AT_SETUP([bigram: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/bigram/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/bigram && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

