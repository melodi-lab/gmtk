
# test and dev model triphone_hmm


AT_SETUP([triphone_hmm: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/triphone_hmm/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/triphone_hmm && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

