
# test and dev model pgm


AT_SETUP([pgm: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/pgm/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/pgm && ulimit -v 8000000 &&            ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

