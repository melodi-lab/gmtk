
# test and dev model john


AT_SETUP([john: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/john/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/john && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

AT_SETUP([john: infocommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/john/infocommand])
AT_CHECK([(cd $TEST_AND_DEV/john && ulimit -v 8000000 && ulimit -t 120 && ./infocommand)], [], [ignore], [ignore])
AT_CLEANUP

AT_SETUP([john: jtcommand])
# too much memory
AT_SKIP_IF([test x"$GMTK_NOSKIP" = x""])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/john/jtcommand])
AT_CHECK([(cd $TEST_AND_DEV/john && ulimit -v 8000000 && ulimit -t 120 && ./jtcommand)], [], [ignore], [ignore])
AT_CLEANUP
