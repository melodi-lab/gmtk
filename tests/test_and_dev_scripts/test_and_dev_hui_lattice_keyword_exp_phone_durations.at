
# test and dev model hui_lattice_keyword/exp_phone_durations


AT_SETUP([hui_lattice_keyword/exp_phone_durations: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/hui_lattice_keyword/exp_phone_durations/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/hui_lattice_keyword/exp_phone_durations && ulimit -v 8000000 &&            ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

