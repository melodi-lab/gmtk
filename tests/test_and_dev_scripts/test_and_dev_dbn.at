
# test and dev model dbn


AT_SETUP([dbn: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/dbn/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/dbn && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

