
# test and dev model medianus


AT_SETUP([medianus: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/medianus/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/medianus && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

AT_SETUP([medianus: infocommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/medianus/infocommand])
AT_CHECK([(cd $TEST_AND_DEV/medianus && ulimit -v 8000000 && ulimit -t 120 && ./infocommand)], [], [ignore], [ignore])
AT_CLEANUP

AT_SETUP([medianus: jtcommand])
# not a valid model
AT_SKIP_IF([true])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/medianus/jtcommand])
AT_CHECK([(cd $TEST_AND_DEV/medianus && ulimit -v 8000000 && ulimit -t 120 && ./jtcommand)], [], [ignore], [ignore])
AT_CLEANUP
