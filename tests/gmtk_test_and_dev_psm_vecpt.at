
# test and dev model psm_vecpt


AT_SETUP([psm_vecpt: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/psm_vecpt/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/psm_vecpt && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

