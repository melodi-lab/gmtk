
# test and dev model gang_mslm


AT_SETUP([gang_mslm: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/gang_mslm/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/gang_mslm && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

