
# test and dev model thad_june


AT_SETUP([thad_june: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/thad_june/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/thad_june && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

