
# test and dev model simple_hmm_2frame_chunk


AT_SETUP([simple_hmm_2frame_chunk: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/simple_hmm_2frame_chunk/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/simple_hmm_2frame_chunk && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

