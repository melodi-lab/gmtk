
# test and dev model snake


AT_SETUP([snake: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/snake/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/snake && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

