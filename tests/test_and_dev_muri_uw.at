AT_SKIP_IF([test ! -x $TEST_AND_DEV/muri_uw/jtcommand])
# https://j.ee.washington.edu/trac/gmtk/ticket/323
AT_XFAIL_IF([true])
AT_CHECK([(cd $TEST_AND_DEV/muri_uw && ulimit -v 8000000 && ulimit -t 120 && ./jtcommand)], [], [ignore], [ignore])
