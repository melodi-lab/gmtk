
# test and dev model random_demo


AT_SETUP([random_demo: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/random_demo/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/random_demo && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

AT_SETUP([random_demo: jtcommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/random_demo/jtcommand])
AT_CHECK([(cd $TEST_AND_DEV/random_demo && ulimit -v 8000000 && ulimit -t 120 && ./jtcommand)], [], [ignore], [ignore])
AT_CLEANUP
