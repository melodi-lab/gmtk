
# test and dev model tagger


AT_SETUP([tagger: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/tagger/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/tagger && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

