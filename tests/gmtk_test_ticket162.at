          gmtkViterbi -strF mouse4.str -inputM mouse4.mtr -of1 \
          inMicePa.lst -fmt1 ascii -nf1 6 -vitValsFile - \
         | grep Ptn-51 | awk '{printf $2}')],[],[C:])
