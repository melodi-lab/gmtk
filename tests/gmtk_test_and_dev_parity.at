
# test and dev model parity


AT_SETUP([parity: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/parity/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/parity && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

