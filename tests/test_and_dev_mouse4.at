
# test and dev model mouse4


AT_SETUP([mouse4: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/mouse4/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/mouse4 && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

