
# test and dev model testLMs


AT_SETUP([testLMs: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/testLMs/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/testLMs && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

