
# test and dev model klivescu


AT_SETUP([klivescu: tricommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/klivescu/tricommand])
AT_CHECK([(cd $TEST_AND_DEV/klivescu && ulimit -v 8000000 && ulimit -t 120 && ./tricommand)], [], [ignore], [ignore])
AT_CLEANUP

AT_SETUP([klivescu: infocommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/klivescu/infocommand])
AT_CHECK([if test -f $TEST_AND_DEV/klivescu/PARAMS/masterFile.Mar9_16.bz2 \
             -a ! -f $TEST_AND_DEV/klivescu/PARAMS/masterFile.Mar9_16;    \
          then (cd $TEST_AND_DEV/klivescu/PARAMS && bunzip2 *.bz2); fi],
          [],[ignore],[ignore])
AT_CHECK([(cd $TEST_AND_DEV/klivescu && ulimit -v 8000000 && ulimit -t 120 && ./infocommand)], [], [ignore], [ignore])
AT_CLEANUP

AT_SETUP([klivescu: jtcommand])
AT_SKIP_IF([test ! -x $TEST_AND_DEV/klivescu/jtcommand])
AT_CHECK([(cd $TEST_AND_DEV/klivescu && ulimit -v 8000000 && ulimit -t 120 && ./jtcommand)], [], [ignore], [ignore])
AT_CLEANUP
